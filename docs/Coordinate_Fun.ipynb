   "source": [
    "\n",
    "from astropy.coordinates import ICRS, FK5, FK4, Galactic, Angle\n",
    "import astropy.units as u\n",
    "import numpy as np\n",
    "ra=Angle(129.729,u.deg)\n",
    "dec=Angle(19.1983,u.deg)\n",
    "sigra=np.cos(19.1983/360.*2.*3.141)\n",
    "print(129.729*sigra)\n",
    "ucac=FK5(ra,dec)\n",
    "print(ucac)\n",
    "ucac.transform_to(ICRS)\n"
   ]
